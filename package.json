{
	"name": "luxio",
	"version": "1.1.7",
	"description": "Luxio Client",
	"main": "src/index.js",
	"scripts": {
		"build": "rm -r dist > /dev/null 2>&1; mkdir dist; browserify src/index.js -o dist/luxio.js -t [ babelify --presets [ env ] ]; uglifyjs --compress --mangle -- dist/luxio.js > dist/luxio.min.js;",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "Emile Nijssen",
	"license": "GPL-3.0",
	"dependencies": {
		"babel-polyfill": "^6.26.0",
		"commander": "^2.13.0",
		"node-fetch": "^1.7.3",
		"timeago.js": "^3.0.2",
		"tinygradient": "^0.3.1"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babelify": "^8.0.0"
	},
	"bin": {
		"luxio": "bin/luxio.js"
	},
	"babel": {
		"presets": [
			[
				"env",
				{
					"targets": {
						"browsers": [
							"last 2 versions"
						],
						"uglify": true
					}
				}
			]
		]
	}
}
